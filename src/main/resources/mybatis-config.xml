<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="kh.spring.gaji.administer.model.dto.AdministerDto" alias="AdministerDto"/>
		<typeAlias type="kh.spring.gaji.administer.model.dto.GajiAccountDto" alias="GajiAccountDto"/>
		<typeAlias type="kh.spring.gaji.category.model.dto.CategoryDto" alias="CategoryDto"/>
		<typeAlias type="kh.spring.gaji.chat.model.dto.ChatMessageDto" alias="ChatMessageDto"/>
		<typeAlias type="kh.spring.gaji.chat.model.dto.ChatRoomDto" alias="ChatRoomDto"/>
		<typeAlias type="kh.spring.gaji.community.model.dto.CommentDto" alias="CommentDto"/>
		<typeAlias type="kh.spring.gaji.community.model.dto.CommunityDto" alias="CommunityDto"/>
		<typeAlias type="kh.spring.gaji.trade.model.dto.DealReviewDto" alias="DealReviewDto"/>
		<typeAlias type="kh.spring.gaji.file.model.dto.FileDto" alias="FileDto"/>
		<typeAlias type="kh.spring.gaji.goods.model.dto.GoodsDto" alias="GoodsDto"/>
		<typeAlias type="kh.spring.gaji.interestcategory.model.dto.InterestCategoryDto" alias="InterestCategoryDto"/>
		<typeAlias type="kh.spring.gaji.notification.model.dto.NotificationDto" alias="NotificationDto"/>
		<typeAlias type="kh.spring.gaji.notification.model.dto.NotificationKeywordDto" alias="NotificationKeywordDto"/>
		<typeAlias type="kh.spring.gaji.trade.model.dto.InFaceTradingDto" alias="InFaceTradingDto"/>
		<typeAlias type="kh.spring.gaji.trade.model.dto.SafeTradingDto" alias="SafeTradingDto"/>
		<typeAlias type="kh.spring.gaji.report.model.dto.ReportDto" alias="ReportDto"/>
		<typeAlias type="kh.spring.gaji.user.model.dto.UserAccountDto" alias="UserAccountDto"/>
		<typeAlias type="kh.spring.gaji.user.model.dto.UserAddressDto" alias="UserAddressDto"/>
		<typeAlias type="kh.spring.gaji.user.model.dto.UserDto" alias="UserDto"/>
		<typeAlias type="kh.spring.gaji.user.model.dto.UserToUserDto" alias="UserToUserDto"/>
		<typeAlias type="kh.spring.gaji.wishlist.model.dto.WishListDto" alias="WishListDto"/>
	</typeAliases>
	
	 <mappers>
		<mapper resource="mappers/admin-mapper.xml"/> 		<!-- 관리자 관련 쿼리문 -->
		<mapper resource="mappers/category-mapper.xml"/>	<!-- 카테고리 관련 쿼리문 -->
		<mapper resource="mappers/chat-mapper.xml"/>		<!-- 채팅관련 쿼리문 -->
		<mapper resource="mappers/community-mapper.xml"/>	<!-- 커뮤니티글 관련 쿼리문 -->
		<mapper resource="mappers/dealreview-mapper.xml"/>	<!-- 리뷰관련 쿼리문 -->
		<mapper resource="mappers/file-mapper.xml"/>	<!-- 파일관련 쿼리문 -->
		<mapper resource="mappers/notification-mapper.xml"/>	<!-- 알림관련 쿼리문 -->
		<mapper resource="mappers/trade-mapper.xml"/>	<!-- 거래 관련 쿼리문 -->
		<mapper resource="mappers/report-mapper.xml"/>	<!-- 신고관련 쿼리문 -->
		<mapper resource="mappers/user-mapper.xml"/> <!-- 유저 관련 쿼리문 -->
		<mapper resource="mappers/goods-mapper.xml"/> <!-- 상품 관련 쿼리문 -->
	</mappers>
</configuration>
